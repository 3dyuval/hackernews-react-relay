{
  "name": "relay-starter",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "concurrently \"npm run relay\" \"vite\"",
    "build": "relay-compiler --validate && tsc && vite build",
    "lint": "eslint src --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "relay": "relay-compiler --watch --output=verbose",
    "tsc": "tsc --noEmit --skipLibCheck"
  },
  "dependencies": {
    "@babel/core": "^7.22.1",
    "@fusionauth/react-sdk": "^0.25.0",
    "concurrently": "^8.1.0",
    "date-fns": "^2.30.0",
    "notistack": "^3.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-relay": "^15.0.0",
    "react-router-dom": "^6.11.1",
    "relay-runtime": "^15.0.0",
    "sass": "^1.63.3",
    "vite-plugin-relay": "^2.0.0"
  },
  "devDependencies": {
    "@swc/cli": "^0.1.62",
    "@swc/core": "^1.3.62",
    "@types/node": "^18.16.3",
    "@types/react": "^18.0.28",
    "@types/react-dom": "^18.0.11",
    "@types/react-relay": "^14.1.3",
    "@types/relay-runtime": "^14.1.10",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitejs/plugin-react-swc": "^3.3.0",
    "autoprefixer": "^10.4.14",
    "babel-plugin-relay": "^14.1.0",
    "eslint": "^8.38.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.3.4",
    "graphql": "^16.6.0",
    "postcss": "^8.4.24",
    "relay-compiler": "^15.0.0",
    "tailwindcss": "^3.3.2",
    "typescript": "^5.1.6",
    "vite": "^4.3.2"
  }
}
